{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2020",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmit": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": false,
    "outDir": "dist",
    "noUnusedParameters": false,
    "baseUrl": "."
  }
}
